{
  "name": "flexkit/ten-backend",
  "description": "FlexKit Ten - PHP Application Backend",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "vlucas/phpdotenv": "^5.0",
    "monolog/monolog": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "FlexkitTen\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public",
    "dev": "php -S localhost:8000 -t public"
  },
  "config": {
    "optimize-autoloader": true
  }
}
