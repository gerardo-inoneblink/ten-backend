{
    "name": "flexkit/ten-backend",
    "description": "flexkit ten backend",
    "type": "project",
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Flexkit\\TenBackend\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "test": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "config": {
        "optimize-autoloader": true
    }
}